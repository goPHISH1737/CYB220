count = 1
maximum = 0

fn = 1
fnplusone = 1
temp = 0

print("This program will show the fibonacci sequence!")

while count <= 100:
    print(f"#{count} of the fibonacci sequence is {fn}")
    temp = fn
    fn = fnplusone
    fnplusone += temp
    count += 1
    user_input = input("Type 'quit' to exit, or press enter to continue: ")
