short_messages = ['Hey', 'Wassup', 'Yeah', 'No']
sent_messages = []

def show_messages(messages):
    for message in messages:
        print(message)

def send_messages(messages):
    for message in messages:
        print(message)
        sent_messages.append(message)


send_messages(short_messages[:])
print(short_messages)
print(sent_messages)
